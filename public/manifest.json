{
    "name": "Spreadsheets Filter",
    "description": "Search and filter your Google Sheets tabs by keyword!",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icon/16.png",
        "32": "icon/32.png",
        "48": "icon/48.png",
        "128": "icon/128.png"
    },
    "content_scripts": [
      {
          "matches": ["https://docs.google.com/spreadsheets/d/*/edit*"],
          "js": ["content.js"],
          "css": ["content.css"]
      }
    ]
 }